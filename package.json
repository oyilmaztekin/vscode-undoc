{
    "name": "undoc",
    "displayName": "undoc",
    "description": "An extention that detects undocumented functions/methods and generates documantation for them.",
    "keywords": [
        "comment-generator",
        "documentation-generator",
        "doc-generator",
        "document-generator",
        "undocumented-functions"
    ],
    "version": "0.0.10",
    "publisher": "undoc",
    "repository": "https://github.com/oyilmaztekin/vscode-undoc",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Formatters",
        "Snippets",
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.createDocsTemplate"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.createDocsTemplate",
                "title": "Create Docs"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "eslint": "^4.11.0",
        "expect.js": "^0.3.1",
        "sinon": "^7.1.1",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "@babel/core": "^7.1.2",
        "@babel/preset-env": "^7.1.0",
        "@babel/plugin-proposal-async-generator-functions": "^7.1.0",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-proposal-optional-catch-binding": "^7.0.0",
        "@babel/plugin-transform-block-scoped-functions": "^7.0.0",
        "@babel/plugin-transform-flow-strip-types": "^7.0.0",
        "@babel/plugin-transform-literals": "^7.0.0",
        "@babel/plugin-transform-object-super": "^7.1.0",
        "@babel/plugin-transform-react-display-name": "^7.0.0",
        "@babel/plugin-transform-react-jsx": "^7.0.0",
        "@babel/preset-flow": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "@babel/parser": "^7.1.3",
        "babel-traverse": "^6.26.0",
        "babel-generator": "^6.26.1"
    }
}
